<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>allure</name>
        <display-name>@Name@</display-name>
        <version>@Version@</version>
        <description>@Description@</description>
        <download-url>@OrganizationUrl@</download-url>
        <email>allure@qameta.io</email>
        <vendor>
            <name>@OrganizationName@</name>
            <url>@OrganizationUrl@</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true"/>
</teamcity-plugin>
