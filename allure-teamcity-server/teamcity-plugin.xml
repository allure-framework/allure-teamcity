<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>allure</name>
        <display-name>Allure for TeamCity</display-name>
        <version>@Version@</version>
        <description>Plugin adds support for generate Allure report based on tests results</description>
        <download-url>https://github.com/allure-framework/allure-teamcity</download-url>
        <email>allure@qameta.io</email>
        <vendor>
            <name>Allure Framework</name>
            <url>https://github.com/allure-framework</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>
